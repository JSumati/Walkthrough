{
	"_version": "1.8.0",
	"sap.app": {
		"id": "Walkthrough",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"invoiceRemote": {
				"uri": "/destinations/Northwind/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "Walkthrough.view.View1",
			"type": "XML",
			"async": true,
			"id": "view1"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {
					"minVersion": ""
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "Walkthrough.i18n.i18n"
				}
			},
			"invoice": {
				"dataSource": "invoiceRemote"
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"resources": {
			"css": [{
				"uri": "css/style.css",
				"id": "custom"
			}]
		},
		"routing": {
			"config": {
				"viewPath": "Walkthrough.view",
				"controlAggregation": "pages",
				"controlId": "walkthrough"
			},
			"routes": [{
				"name": "route name",
				"pattern": "",
				"titleTarget": "",
				"greedy": false
			}]
		}
	}
}